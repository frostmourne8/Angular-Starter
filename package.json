{
  "name": "angular-starter",
  "version": "0.0.0",
  "description": "A base project to use for future angular applications",
  "scripts": {
    "start": "concurrently --kill-others --raw \"npm run startServer\" \"npm run startDev\"",
    "startServer": "node server/server.js",
    "startDev": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress",
    "test": "karma start tests/karma.conf.js",
    "test:single": "karma start tests/karma.conf.js --single-run",
    "build": "rimraf server/public && webpack --config webpack/webpack.prod.js --progress --profile --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frostmourne8/Angular-Starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.6.0",
    "angular-resource": "1.6.1",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "0.3.2",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "express": "4.14.0",    
    "jquery": "3.1.1",
    "lodash": "4.16.6",
    "request": "2.78.0",
    "request-promise": "4.1.1",
    "serve-favicon": "2.3.0"
  },
  "devDependencies": {
    "@types/angular": "1.5.22",
    "@types/angular-mocks": "1.5.6",
    "@types/angular-resource": "1.5.8",
    "@types/angular-ui-router": "1.1.35",
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.36",
    "@types/lodash": "4.14.38",
    "@types/node": "6.0.46",
    "angular-mocks": "1.6.1",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "concurrently": "3.1.0",
    "css-loader": "0.25.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",    
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.0",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jquery": "3.1.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "ng-annotate-loader": "^0.2.0",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.7.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "tslint": "3.15.1",
